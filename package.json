{
  "name": "modev",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "css": "postcss -u autoprefixer -r dist/css/*.css",
    "sass": "node-sass --output-style compressed src/sass/styles.scss dist/css/styles.css && npm run css",
    "sass:w": "nodemon -e scss -x \"npm run sass\"",
    "js": "webpack -p -w",
    "start": "concurrently \"npm run sass:w\" \"npm run js\" \"node index.js\""
  },
  "babel": {
    "presets": [
      "babel-preset-env"
    ]
  },
  "author": "Jon Leibham (bhamdesigns.com)",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-env": "^1.1.8",
    "concurrently": "^2.2.0",
    "lodash": "^4.17.4",
    "node-sass": "^3.12.2",
    "postcss-cli": "^2.5.2"
  },
  "dependencies": {
    "express": "^4.14.0",
    "hbs": "^4.0.1",
    "webpack": "^2.2.1"
  }
}
